@using ProyectoFinal.Models;
@model FacturaDto

@{

    Layout = null;
    ViewBag.Title = "Creación Factura";
}

@if (Model != null)
{

    <div class="container">

        <div class="row">
            <div class="col">
                <h2>@ViewBag.Title</h2>

                @using (Html.BeginForm("ProcesoFact", "GenerarFactura", FormMethod.Post))
                {
                <input type="submit" />
                }

            </div>
            
            <div class="row">
                <div class="col">
                   footer
                </div>
               
            </div>
        </div>
            

    </div>

}
else
{ @Html.Raw("EL modelo no fue instanciado");
}
